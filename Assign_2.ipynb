    "class Solution {\n",
    "public:\n",
    "    int distributeCandies(vector<int>& candyType) {\n",
    "        int n = candyType.size();\n",
    "        set<int>s;\n",
    "        for(auto i : candyType) s.insert(i);\n",
    "        int m = s.size() ;\n",
    "        return min (n/2 , m);\n",
    "    }\n",
    "};"
   ]
